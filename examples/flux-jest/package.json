{
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --no-info --inline --progress",
    "test": "jsxhint . && node ../../bin/jest.js"
  },
  "dependencies": {
    "bluebird": "latest",
    "flux": "latest",
    "immutable": "latest",
    "object-assign": "latest",
    "react": "latest",
    "react-tools": "latest"
  },
  "devDependencies": {
    "babel-jest": "latest",
    "babel-loader": "latest",
    "json-loader": "latest",
    "jsx-loader": "latest",
    "jsxhint": "latest",
    "webpack-dev-server": "latest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-tools",
      "<rootDir>/node_modules/immutable"
    ],
    "moduleFileExtensions": ["js", "jsx", "json"]
  }
}

