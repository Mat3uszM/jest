{
  "type": "module",
  "devDependencies": {
    "discord.js": "14.3.0",
    "yargs": "^17.5.1"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "\\.m?[jt]sx?$": "babel-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.(m)[jt]s?(x)",
      "**/?(*.)+(spec|test).(m)?[tj]s?(x)"
    ]
  }
}
