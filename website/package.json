{
  "name": "jest-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus-start",
    "build": "docusaurus-build",
    "gh-pages": "echo 'replaced by Netlify deployment!'",
    "examples": "docusaurus-examples",
    "crowdin-upload": "java -jar crowdin-cli-v2.jar --config ../crowdin.yaml upload sources --auto-update -b master",
    "crowdin-download": "java -jar crowdin-cli-v2.jar --config ../crowdin.yaml download -b master",
    "publish-gh-pages": "echo 'replaced by Netlify deployment!'",
    "write-translations": "docusaurus-write-translations",
    "version": "docusaurus-version",
    "rename-version": "docusaurus-rename-version",
    "netlify:archive": "yarn write-translations && yarn crowdin-download && node fetchSupporters.js && yarn build"
  },
  "dependencies": {
    "docusaurus": "^1.14.2"
  },
  "devDependencies": {
    "graphql": "^15.3.0",
    "graphql-request": "^3.1.0"
  }
}
